# Converting nanopore fast5 data to pod5
conda activate py39
pip install pod5

# made temporary directories for copies of all the fast5 files
mkdir tmp_fast5
mkdir tmp_pod5_all

# then converted all fast5 copies to pod5:
pod5 convert fast5 ./tmp_fast5_all/ --output ./tmp_pod5_all --one-to-one ./tmp_fast5_all



